{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "data_objects/merchant",
    "title": "Merchant",
    "description": "A Versa merchant returned by the registry, associated with the sender client_id",
    "type": "object",
    "required": ["id", "name", "brand_color", "logo"],
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "brand_color": {
        "type": "string",
        "minLength": 7,
        "maxLength": 7,
        "pattern": "^#(?:[0-9a-fA-F]{3}){1,2}$"
      },
      "logo": {
        "type": "string",
        "format": "uri"
      }
    }
}